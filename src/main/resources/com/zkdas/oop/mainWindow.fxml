                      <Tab fx:id="containerTabItems" text="Items" />
                      <Tab fx:id="containerTabCustomers" text="Customers" />
                  <Tab fx:id="containerTabCarts" text="Carts" />
                  <Tab fx:id="containerTabOrders" text="Orders" />
                  </tabs>
              </TabPane>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="Test">
                  <items>
                    <MenuItem fx:id="menuGenerateElement" mnemonicParsing="false" onAction="#menuGenerateElementClick" text="generate element" />
                        <MenuItem fx:id="menuGenerateElements" mnemonicParsing="false" onAction="#menuGenerateElementsClick" text="generate 10 elements" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </GridPane>

    </children>
</AnchorPane>
